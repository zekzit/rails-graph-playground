<!DOCTYPE html>
<html>
  <head>
    <title>InventoryJournal</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div style="float:right">
      Current user: <%= session[:current_user].present? ? session[:current_user] : "N/A" %>
    </div>
    <%= yield %>
  </body>
</html>
