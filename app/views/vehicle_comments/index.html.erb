<p id="notice"><%= notice %></p>

<h1>Vehicle Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_comments.each do |vehicle_comment| %>
      <tr>
        <td><%= link_to 'Show', vehicle_comment %></td>
        <td><%= link_to 'Edit', edit_vehicle_comment_path(vehicle_comment) %></td>
        <td><%= link_to 'Destroy', vehicle_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle Comment', new_vehicle_comment_path %>
